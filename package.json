{
  "name": "bomarkdown-color-theme",
  "displayName": "BoMarkdown Color Theme",
  "description": "Color theme and associated grammar for BoMarkdown syntax",
  "icon": "images/ExtensionIcon.png",
  "version": "0.1.1",
  "publisher": "antpri",
  "engines": {
    "vscode": "^1.96.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antprin/bomarkdown-color-theme.git"
  },
  "categories": [
    "Themes",
    "Snippets"
  ],
  "contributes": {
    "themes": [
      {
        "label": "BoMarkdown Color Theme",
        "uiTheme": "vs-dark",
        "path": "./src/themes/bomarkdown-color-theme.json"
      }
    ],
    "grammars": [
      {
        "language": "markdown",
        "scopeName": "source.bomarkdowngrammar",
        "path": "./src/syntaxes/bomarkdown-grammar.json"
      }
    ],
    "snippets": [
      {
        "language": "markdown",
        "path": "./src/snippets/snippets.code-snippets"
      }
    ]
  }
}